<h2>Edit product</h2>

<%= form_for @product do |f| %>

<%= f.label :vendor_id %>
<%= f.text_field :vendor_id %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :inventory %>
  <%= f.text_field :inventory %>


  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.submit %>
<% end %>

<%= link_to "Delete!", @product, method: :delete, class: "delete" %>
