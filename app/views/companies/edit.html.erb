<h2>Edit Company</h2>
<%= form_for @company do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :logo_url %>
  <%= f.text_field :logo_url %>

  <%= f.submit %>
<% end %>

<%= link_to "Delete", @company, method: :delete, class: "delete" %>
